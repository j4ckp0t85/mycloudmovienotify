set from = "sender_email@gmail.com"
set use_from = yes
set envelope_from = yes

set smtp_url = "smtps://sender_email@gmail.com@smtp.gmail.com:465/"
set smtp_pass = “2_factor_pwd”
set imap_user = "sender_email@gmail.com"
set imap_pass = "2_factor_pwd"
set folder = "imaps://imap.gmail.com:993"
set spoolfile = "+INBOX"
set ssl_force_tls = yes

# G to get mail
bind index G imap-fetch-mail
set editor = “nano”
set charset = "utf-8"
set record = ''
